@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-wechat:before {
  content: "\e637";
  color: #00c800;
}

.icon-moments:before {
  content: "\e69a";
  color: #07c160;
}

.weui-mask {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.8);
}

.weui-actionsheet {
  position: fixed;
  left: 0;
  bottom: 0;
  transform: translate(0, 100%);
  backface-visibility: hidden;
  z-index: 5000;
  width: 100%;
  background-color: #fff;
  transition: transform 0.3s;
  border-top-left-radius: 12px;
  border-top-right-radius: 12px;
  overflow: hidden;
}

.weui-actionsheet_toggle {
  transform: translate(0, 0);
}

.weui-flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.weui-flex__item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.weui-loading {
  width: 40px;
  height: 40px;
  margin: 10px auto;
  vertical-align: middle;
  align-items: center;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.share-dialog {
  visibility: hidden;
  opacity: 0;
  transition: all 0.3s;
}

.share-dialog_show {
  visibility: visible;
  opacity: 1;
}

.icon-wechat, .icon-moments {
  font-size: 30px;
  font-weight: 300;
}

.share-meta {
  font-size: 11px;
  color: #888;
  font-weight: 300;
}

.placeholder {
  text-align: center;
}

.btn {
  background: #fff;
}

.fadeIn {
  -webkit-animation: c 0.3s forwards;
  animation: c 0.3s forwards;
}

@keyframes c {
  0% {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

.weui-dialog {
  position: fixed;
  z-index: 5000;
  top: 54%;
  left: 50rpx;
  right: 50rpx;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  background-color: var(--weui-BG-2);
  text-align: center;
  border-radius:20px;
  overflow: hidden;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: column;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  flex-direction: column;
  max-height: 90%;
}

.weui-dialog__ft {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
  font-size: 17px;
}

.weui-dialog__ft:after {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid var(--weui-DIALOG-LINE-COLOR);
  color: var(--weui-DIALOG-LINE-COLOR);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

.weui-dialog__btn_primary {
  color: #07c160;
	line-height: 2;
  background: #fff;
  border-radius: 0;
}
.weui-dialog__btn_default {
  line-height: 2;
  background: #fff;
  border-radius: 0;
}